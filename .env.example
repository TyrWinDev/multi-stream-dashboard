PORT=3000
CLIENT_URL=http://localhost:5173

# Twitch
TWITCH_CHANNEL=your_channel_name
# Required for OAuth:
TWITCH_CLIENT_ID=your_client_id
TWITCH_CLIENT_SECRET=your_client_secret
# Redirect URI should be: http://localhost:3000/api/auth/twitch/callback

# TikTok
TIKTOK_USER=your_username

# YouTube
YOUTUBE_CHANNEL_ID=UC...
# Required for OAuth:
YOUTUBE_CLIENT_ID=your_client_id
YOUTUBE_CLIENT_SECRET=your_client_secret
# Redirect URI should be: https://localhost:3001/api/auth/youtube/callback

# Kick
KICK_CHANNEL_ID=your_kick_slug
# Required for OAuth:
KICK_CLIENT_ID=your_client_id
KICK_CLIENT_SECRET=your_client_secret
# Redirect URI should be: http://localhost:3000/api/auth/kick/callback

# StreamElements (Get JWT from Dashboard -> Profile)
STREAMELEMENTS_JWT=eyJ...
